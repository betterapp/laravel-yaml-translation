{
  "name": "betterapp/laravel-yaml-translation",
  "description":
    "Add YAML file support to Laravel TranslationServiceProvider, fork of jackjoe/yaml-translation",
  "keywords": ["laravel", "translations", "yaml"],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Czestki",
      "email": "biuro@betterapp.net"
    },
    {
      "name": "Jack + Joe",
      "email": "hello@jackjoe.be"
    },
    {
      "name": "Thomas SIEFFERT",
      "email": "caporaldead@devitek.fr"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/translation": "^10.0",
    "symfony/yaml": ">=4.0"
  },
  "autoload": {
    "psr-4": {
      "betterapp\\LaravelYamlTranslation\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["betterapp\\LaravelYamlTranslation\\TranslationServiceProvider"]
    }
  }
}
